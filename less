[m     [90m [39m     [36m...[90m([36mfontSize[90m ?[39m [90m{[39m [36mfontSize[90m }[39m [36m:[90m {}),
[m     [90m [39m   [90m}}>
[m           {/* Optional card wrapper for Love2D */}
[m           [90m<[34mBox[90m [36mstyle[90m=[31m{{
[m     [90m [39m       [36mflexDirection:[90m '[36mrow[90m',
[m     [90m [39m       [36mgap:[90m [36m2[90m *[39m [36mcell[90m,
[m     [90m [39m       [36m...[90m([36misPixel[90m ?[39m [90m{
[m     [90m [39m         [36mbackgroundColor:[90m '#[36m0e0e18[90m',
[m     [90m [39m         [36mborderRadius:[90m [36mcell[90m,
[m     [90m [39m         [36mborderWidth:[90m [36m1[90m,
[m     [90m [39m         [36mborderColor:[90m '#[36m1a1a2e[90m',
[m     [90m [39m         [36mpadding:[90m [36m2[90m *[39m [36mcell[90m,
[m     [90m [39m       [90m}[39m [36m:[90m {}),
[m     [90m [39m     [90m}}>
[m             [90m<[34mHeart[90m [36mcell[90m=[31m{cell}[90m />
[m             [90m<[34mBox[90m [36mstyle[90m=[31m{{[90m [36mflexDirection:[90m '[36mcolumn[90m',[39m [36mgap:[90m [36mMath.round[90m([36mcell[90m *[39m [36m0.3[90m),[39m [36mflexGrow:[90m [36m1[90m }}>
[m               [90m<[34mText[90m [36mstyle[90m=[31m{{[90m [36mcolor:[90m [36mACCENT[90m,[39m [36mfontWeight:[90m '[36mbold[90m'[39m [90m}}>[39m{title}[90m</[34mText[90m>
