[m       );
[m     }

[m     [38:2:177:185:249mHEART_GRID[39m is derived from [38:2:177:185:249mHEART_LINES[39m â€” a boolean[][] where [38:2:177:185:249mtrue[39m = filled cell:
[m     [34mconst[39m HEART_GRID = HEART_LINES.map([33mline[39m [33m=>
[m       [...line].map([33mch[39m [33m=>[39m ch !== [31m'[39m [31m'[39m)
[m     );

[m     - [22;1mTerminal[22m (cell=1): 13Ã—10 colored cells â€” same visual as [38:2:177:185:249mâ–ˆ[39m characters
[m     - [22;1mLove2D[22m (cell=12): 156Ã—120 pixel colored rectangles â€” clean heart graphic

[m     [22;1mPalette:[22m [22;1mBox[22m [22;1melements[22m [22;1m(not[22m [22;1mtext)

[m     [34mfunction[33m ColorPalette({[39m [33mcell[39m [33m}:[39m [33m{[39m [33mcell:[39m [36mnumber[33m })[39m {
[m       [34mreturn[39m (
[m         [90m<[34mBox[90m [36mstyle[90m=[31m{{[90m [36mflexDirection:[90m '[36mrow[90m',[39m [36mgap:[90m [36mMath.round[90m([36mcell[90m *[39m [36m0.25[90m)[39m [90m}}>
[m           {PALETTE.map((color, i) => (
[m             [90m<[34mBox[90m [36mkey[90m=[31m{i}[90m [36mstyle[90m=[31m{{
[m     [90m [39m         [36mwidth:[90m [36mcell[90m *[39m [36m3[90m,
[m     [90m [39m         [36mheight:[90m [36mcell[90m,
[m     [90m [39m         [36mbackgroundColor:[90m [36mcolor[90m,
[m     [90m [39m       [90m}}[39m [90m/>
[m           ))}
[m         [90m</[34mBox[90m>
[m       );
[m     }

[m     [22;1mSeparator:[22m [22;1mBox[22m [22;1mwith[22m [22;1mbackgroundColor[22m [22;1m(not[22m [22;1mtext[22m [22;1mdashes)

[m     [34mfunction[33m Separator({[39m [33mcell[39m [33m}:[39m [33m{[39m [33mcell:[39m [36mnumber[33m })[39m {
[m       [34mreturn[39m [90m<[34mBox[90m [36mstyle[90m=[31m{{[90m [36mheight:[90m [36mMath.max[90m([36m1[90m,[39m [36mMath.round[90m([36mcell[90m *[39m [36m0.08[90m)),[39m [36mbackgroundColor:[90m [36mDIM[90m }}[39m [90m/>[39m;
[m     }

[m     [22;1mInfoLine:[22m [22;1mScaled[22m [22;1mlabel[22m [22;1mwidth

[m     [34mfunction[33m InfoLine({[39m [33mlabel,[39m [33mvalue,[39m [33mcell[39m [33m}:[39m [33m{[39m [33mlabel:[39m [36mstring[33m;[39m [33mvalue:[39m [36mstring[33m;[39m [33mcell:[39m [36mnumber[33m })[39m {
[m       [34mreturn[39m (
[m         [90m<[34mBox[90m [36mstyle[90m=[31m{{[90m [36mflexDirection:[90m '[36mrow[90m',[39m [36mgap:[90m [36mMath.round[90m([36mcell[90m *[39m [36m0.5[90m)[39m [90m}}>
[m           [90m<[34mBox[90m [36mstyle[90m=[31m{{[90m [36mwidth:[90m [36m9[90m *[39m [36mcell[90m }}>
[m             [90m<[34mText[90m [36mstyle[90m=[31m{{[90m [36mcolor:[90m [36mACCENT[90m,[39m [36mfontWeight:[90m '[36mbold[90m'[39m [90m}}>[39m{label}:[90m</[34mText[90m>
[m           [90m</[34mBox[90m>
[m           [90m<[34mBox[90m [36mstyle[90m=[31m{{[90m [36mflexGrow:[90m [36m1[90m }}>
[m             [90m<[34mText[90m [36mstyle[90m=[31m{{[90m [36mcolor:[90m [36mBRIGHT[90m }}>[39m{value}[90m</[34mText[90m>
[m           [90m</[34mBox[90m>
[m         [90m</[34mBox[90m>
[m       );
[m     }

[m     [22;1mMain[22m [22;1mlayout:[22m [22;1mNo[22m [22;1;38:2:177:185:249mjustifyContent:[22;39m [22;1;38:2:177:185:249m'center'[39m,[22m [22;1mproper[22m [22;1mscaling

[m     [34minterface[39m AppProps {
[m       [36minfo[39m: SystemInfo;
[m       cell?: [36mnumber[39m;       [32m//[39m [32m1[39m [32mfor[39m [32mterminal,[39m [32m~12[39m [32mfor[39m [32mLove2D
[m       fontSize?: [36mnumber[39m;   [32m//[39m [32mundefined[39m [32mfor[39m [32mterminal,[39m [32m14[39m [32mfor[39m [32mLove2D
[m       showFps?: [36mnumber[39m;    [32m//[39m [32moptional[39m [32mFPS[39m [32mcounter[39m [32m(Love2D[39m [32monly)
[m     }

[m     [34mexport[39m [34mdefault[39m [34mfunction[33m App({[39m [33minfo,[39m [33mcell[39m [33m=[39m [32m1[33m,[39m [33mfontSize,[39m [33mshowFps[39m [33m}:[39m [33mAppProps)[39m {
