[m     [34mimport[39m React, { useState, useEffect } [34mfrom[39m [31m'react'[39m;
[m     [34mimport[39m { createTerminalApp } [34mfrom[39m [31m'@ilovereact/terminal'[39m;
[m     [34mimport[39m { getSystemInfo } [34mfrom[39m [31m'./sysinfo'[39m;
[m     [34mimport[39m App [34mfrom[39m [31m'./App'[39m;

[m     [34mfunction[33m TerminalNeofetch()[39m {
[m       [34mconst[39m [info, setInfo] = useState(getSystemInfo());
[m       useEffect([33m()[39m [33m=>[39m {
[m         [34mconst[39m t = [36msetInterval[39m([33m()[39m [33m=>[39m setInfo(getSystemInfo()), [32m30_000[39m);
[m         [34mreturn[39m [33m()[39m [33m=>[39m [36mclearInterval[39m(t);
[m       }, []);
[m       [34mreturn[39m [90m<[34mApp[90m [36minfo[90m=[31m{info}[90m />[39m;
[m     }

[m     [34mconst[39m app = createTerminalApp({ [36mfps[39m: [32m30[39m, [36mfullscreen[39m: [34mtrue[39m });
[m     app.render([90m<[34mTerminalNeofetch[90m />[39m);

